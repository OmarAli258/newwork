function Sys.init 0
    push constant 4
    push constant 2
    call Sys.min 2
    push constant 4
    push constant 2
    call Sys.max 2
    push constant 1000
    push constant 5
    call Sys.range 2
    return

function Sys.min 0
    pop temp 0
    pop temp 1
    push temp 1
    push temp 0
    lt
    if-goto LESSER
    push temp 0
    return
label LESSER
    push temp 1
    return

function Sys.max 0
    pop temp 0
    pop temp 1
    push temp 1
    push temp 0
    gt
    if-goto GREATER
    push temp 0
    return
label GREATER
    push temp 1
    return

function Sys.range 2
    push argument 1
    push constant 1
    lt
    if-goto RETURN_ZERO
    push argument 0
    pop pointer 1
    push that 0
    pop local 0
    push that 0
    pop local 1
    push constant 1
    pop temp 0
label LOOP
    push temp 0
    push argument 1
    eq
    if-goto END_LOOP
    push argument 0
    push temp 0
    add
    pop pointer 1
    push that 0
    push local 0
    call Sys.min 2
    pop local 0
    push local 1
    call Sys.max 2
    pop local 1
    push temp 0
    push constant 1
    add
    pop temp 0
    goto LOOP
label END_LOOP
    push local 1
    push local 0
    sub
    return
label RETURN_ZERO
    push constant 0
    return